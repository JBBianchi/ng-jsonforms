{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "packages",
  "projects": {
    "angular": {
      "projectType": "library",
      "root": "packages/angular",
      "sourceRoot": "packages/angular/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/angular/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/angular/tsconfig.lib.json"
            },
            "esm": {
              "tsConfig": "packages/angular/tsconfig.json"
            },
            "cjs": {
              "tsConfig": "packages/angular/tsconfig.cjs.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/angular/src/test.ts",
            "tsConfig": "packages/angular/tsconfig.spec.json",
            "karmaConfig": "packages/angular/karma.conf.js"
          }
        }
      }
    },
    "angular-material": {
      "projectType": "library",
      "root": "packages/angular-material",
      "sourceRoot": "packages/angular-material/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/angular-material/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/angular-material/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/angular-material/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/angular-material/src/test.ts",
            "tsConfig": "packages/angular-material/tsconfig.spec.json",
            "karmaConfig": "packages/angular-material/karma.conf.js"
          }
        }
      }
    },
    "angular-test": {
      "projectType": "library",
      "root": "packages/angular-test",
      "sourceRoot": "packages/angular-test/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "packages/angular-test/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/angular-test/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/angular-test/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/angular-test/src/test.ts",
            "tsConfig": "packages/angular-test/tsconfig.spec.json",
            "karmaConfig": "packages/angular-test/karma.conf.js"
          }
        }
      }
    }
  }
}
